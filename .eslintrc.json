{
  "env": {
    "es6": true,
    "node": true,
    "mocha": true
  },
  "extends": "airbnb-base",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": "off",
    "no-plusplus": "off",
    "no-return-assign": "off",
    "no-param-reassign": "off",
    "camelcase": "off",
    "no-underscore-dangle": "off",
    "import/prefer-default-export": "off"
  }
}
